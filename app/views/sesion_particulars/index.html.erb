<p id="notice"><%= notice %></p>

<h1>Sesion Particulars</h1>

<table>
  <thead>
    <tr>
      <th>Cita</th>
      <th>Descripcion sesion</th>
      <th>Numero cita</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paciente.sesion_particulars.each do |sesion_particular| %>
      <tr>
        <td><%= sesion_particular.cita %></td>
        <td><%= sesion_particular.descripcion_sesion %></td>
        <td><%= sesion_particular.numero_cita %></td>
        <td><%= link_to 'Show', sesion_particular %></td>
        <td><%= link_to 'Edit', edit_sesion_particular_path(sesion_particular) %></td>
        <td><%= link_to 'Destroy', sesion_particular, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sesion Particular',  new_paciente_sesion_particular_path(@paciente) %>
